AAA,CAAA,CAAA,EAAA,SAAA,GAAA,EAAA,CAAA;;AAEA,qBAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,CAAA,UAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,kBAAA,CADA,GACA,KAAA,CAAA,UAAA,CAAA,CAAA,EAAA,UAAA,CAAA,MAAA,CAAA,CAAA,EAAA,EAAA,CADA;AAEA,kBAAA,CAFA,GAEA,CAAA,CAAA,KAAA,CAAA,GAAA,CAFA;AAGA,kBAAA,KAAA,CAAA,UAAA,CAAA,CAAA,EAAA,UAAA,GAAA,CAAA,CAAA,CAAA,CAAA,GAAA,GAAA,GAAA,CAAA,CAAA,CAAA,CAAA,GAAA,GAAA,GAAA,CAAA,CAAA,CAAA,CAAA;AACA;;AAhBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBA,KApBA;AAqBA,IAAA,aArBA,yBAqBA,OArBA,EAqBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,CAAA,EAAA,CAAA,EADA,CAEA;;AACA,gBAAA,QAHA,GAGA,IAAA,QAAA,EAHA;AAIA,gBAAA,QAAA,CAAA,MAAA,CAAA,aAAA,EAAA,OAAA,GAAA,CAAA;AACA,gBAAA,QAAA,CAAA,MAAA,CAAA,YAAA,EAAA,MAAA,CAAA,UAAA;AALA;AAAA,uBAMA,QAAA,CAAA,IAAA,CAAA,QAAA,CANA;;AAAA;AAMA,gBAAA,GANA;AAAA;AAAA,uBAQA,GAAA,CAAA,IAAA,CAAA,MARA;;AAAA;AAQA,gBAAA,MAAA,CAAA,UARA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA;AA9BA;AA5BA,CAAA","sourcesContent":["<template>\n  <div id=\"list\" class=\"container m-6\">\n    <h2>공지사항</h2>\n    <div class=\"list\">\n      <ul class=\"select-list\">\n        <li @click=\"noticeType = 0\" :class=\"{ active: noticeType == 0 }\">\n          전체\n        </li>\n        <li @click=\"noticeType = 1\" :class=\"{ active: noticeType == 1 }\">\n          공지\n        </li>\n        <li @click=\"noticeType = 2\" :class=\"{ active: noticeType == 2 }\">\n          이벤트\n        </li>\n        <li @click=\"noticeType = 3\" :class=\"{ active: noticeType == 3 }\">\n          보도자료\n        </li>\n      </ul>\n      <div class=\"content-container\">\n        <listDetail :datas=\"noticeList\" />\n        <div class=\"pagination-container\">\n          <paginate\n            :page-count=\"this.pageCount\"\n            :page-range=\"3\"\n            :margin-pages=\"2\"\n            :click-handler=\"clickCallback\"\n            :prev-text=\"'<'\"\n            :next-text=\"'>'\"\n            :container-class=\"'pagination'\"\n            :page-class=\"'page-item'\"\n          ></paginate>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport Paginate from \"vuejs-paginate\";\nimport { listPage } from \"../api/index\";\nimport listDetail from \"../components/list/listDetail\";\n\nexport default {\n  name: \"noticeList\",\n  props: {},\n  components: {\n    listDetail,\n    Paginate\n  },\n  // 초기화, 기본 값 설정\n  data() {\n    return {\n      requestPage: 0,\n      noticeType: 0,\n      noticeList: [],\n      pageCount: 0\n    };\n  },\n  watch: {\n    noticeType() {\n      this.pageCount = 0;\n      this.fetchData();\n    },\n    requestPage() {\n      this.fetchData();\n    }\n  },\n  created() {\n    this.fetchData();\n  },\n  methods: {\n    // fetchData를 click 이벤트에 넣어주고, 인자 안에는 해당 index를 넣어준다.\n    // 정의 내용에서는 셀렉 할 때 마다 바뀌는 컨텐츠(axios로 갖고 오는 값)을 넣어주면 된다.\n    async fetchData() {\n      let formData = new FormData();\n      formData.append(\"requestPage\", this.requestPage);\n      formData.append(\"noticeType\", this.noticeType);\n\n      // API list(formData);\n      const response = await listPage.list(formData);\n      this.noticeList = response.data.result;\n\n      // 페이지네이션 번호 범위 지정\n      this.pageCount = Math.ceil(this.noticeList[0].noticeCnt / 10);\n\n      for (let i = 0; i < this.noticeList.length; i++) {\n        let a = this.noticeList[i].createDate.substr(0, 10);\n        let b = a.split(\"-\");\n        this.noticeList[i].createDate = b[0] + \".\" + b[1] + \".\" + b[2];\n      }\n    },\n    async clickCallback(pageNum) {\n      window.scrollTo(0, 0);\n      // listpage는 api -> index.js에서 받아온 변수고, list는 해당 변수 안에 있던 함수.\n      let formData = new FormData();\n      formData.append(\"requestPage\", pageNum - 1);\n      formData.append(\"noticeType\", this.noticeType);\n      const res = await listPage.list(formData);\n      // 초기값으로 설정한 items에 res.data.object를 담아준다.\n      this.noticeList = await res.data.result;\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\">\n.container {\n  width: 750px;\n  margin: 0 auto;\n  color: #1f2125;\n  h2 {\n    margin: 69px 0 45px 0;\n    font-size: 42px;\n    font-weight: bold;\n    line-height: 1.38;\n    text-align: center;\n  }\n\n  .list {\n    .select-list {\n      display: flex;\n\n      li {\n        margin-right: 20px;\n        padding-bottom: 15px;\n        cursor: pointer;\n\n        &.active {\n          font-weight: bold;\n          border-bottom: 4px solid #50b0b1;\n        }\n      }\n    }\n\n    .content-container {\n      border-top: solid 1px #e5e5e5;\n      .pagination-container {\n        margin: 68px 0 207px 0;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n\n        .pagination {\n          display: flex;\n\n          .page-item {\n            padding: 0 10px;\n            color: #aaaaaa;\n          }\n\n          .active {\n            font-weight: 900;\n            color: #2e3032;\n          }\n        }\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}