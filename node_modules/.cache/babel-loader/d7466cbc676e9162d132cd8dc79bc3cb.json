BAOA,WAAA,CAAA,IAAA,CAAA,QAAA,CAPA;;AAAA;AAOA,gBAAA,MAPA;AAQA,gBAAA,KAAA,CAAA,QAAA,GAAA,MAAA,CAAA,IAAA,CAAA,MAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAhBA;AAiBA,IAAA,WAAA,EAAA,uBAAA;AACA,UAAA,QAAA,GAAA,QAAA,CAAA,cAAA,CAAA,KAAA,CAAA;AACA,WAAA,OAAA,GACA,QAAA,CAAA,OAAA,CAAA,QAAA,CAAA,cAAA,CAAA,KAAA,EAAA,aAAA,EAAA,KADA;AAEA,MAAA,OAAA,CAAA,GAAA,CAAA,UAAA,EAAA,KAAA,OAAA;AACA;AAtBA;AAzBA,CAAA","sourcesContent":["<template>\n  <section>\n    <div class=\"top\">\n      <div>약관 및 정책</div>\n      <ul>\n        <li\n          v-for=\"(category, i) in categorys\"\n          :key=\"i\"\n          @click=\"selectCategory(i + 1)\"\n          :class=\"{active: i == 0}\"\n        >{{ category }}</li>\n      </ul>\n    </div>\n    <div class=\"content-container\">\n      <select id=\"num\" @change=\"selectTerms()\">\n        <option v-for=\"(version, i) in versions\" :key=\"i\" :value=\"i\" selected>{{ version.version }}</option>\n      </select>\n      <div class=\"content\">{{ this.content[this.termsNo].contents }}</div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport { policy } from \"../api/index\";\nimport { versionList } from \"../api/index\";\n\nexport default {\n  name: \"policy\",\n  components: {},\n  data() {\n    return {\n      categorys: [\n        \"서비스 이용약관\",\n        \"유료서비스 이용약관\",\n        \"개인정보 처리방침\"\n      ],\n      categoryNo: 1,\n      termsNo: 0,\n      content: [],\n      versions: []\n    };\n  },\n  created() {\n    this.fetchData();\n  },\n  watch: {\n    categoryNo() {\n      this.fetchData();\n    },\n    termsNo() {}\n  },\n  methods: {\n    selectCategory: function(i) {\n      this.categoryNo = i;\n      let a = this.categorys.splice(i - 1, 1);\n      this.categorys.unshift(a[0]);\n    },\n    // form-data형식으로 데이터를 담아서 백한테 보내준다.\n    async fetchData() {\n      console.log(this.catrgorys);\n      var formData = new FormData();\n      formData.append(\"categoryNo\", this.categoryNo);\n      const res = await policy.list(formData);\n      this.content = res.data.result;\n      // version list data\n      const resVer = await versionList.list(formData);\n      this.versions = resVer.data.result;\n    },\n    selectTerms: function() {\n      var numCheck = document.getElementById(\"num\");\n      this.termsNo =\n        numCheck.options[document.getElementById(\"num\").selectedIndex].value;\n      console.log(\"termsNo2\", this.termsNo);\n    }\n  }\n};\n</script>\n\n<style scoped lang=\"scss\">\nsection {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  width: 750px;\n  margin: 57px auto 0;\n\n  .top {\n    width: 100%;\n\n    div {\n      height: 38px;\n      font-size: 32px;\n      font-weight: bold;\n      color: #2e3032;\n    }\n\n    ul {\n      display: flex;\n      padding: 26px 0 32px;\n      border-bottom: solid 1px #e5e5e5;\n\n      li {\n        width: auto;\n        height: 20px;\n        font-size: 17px;\n        font-weight: bold;\n        color: #2e3032;\n        margin-right: 22px;\n        cursor: pointer;\n\n        &.active {\n          color: #e73c3e;\n        }\n      }\n    }\n  }\n\n  .content-container {\n    width: 100%;\n\n    select {\n      width: 126px;\n      height: 34px;\n      font-size: 15px;\n      color: #2e3032;\n      border-radius: 4px;\n      border: none;\n      background-color: #f2f2f2;\n      margin: 33px 0 36px;\n      text-indent: 18px;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}